{
    "name": "FulbertGato/SessionManager",
    "type": "library",
    "description": "a lightweight php session manager",
    "keywords": [
        "FulbertGato",
        "SessionManager"
    ],
    "homepage": "https://github.com/FulbertGato/SessionManager",
    "license": "MIT",
    "authors": [
        {
            "name": "Gato Junior",
            "email": "gatojunior.co@gmail.com",
            "homepage": "https://gatojunior.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "FulbertGato\\SessionManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FulbertGato\\SessionManager\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
